#ifndef TREENODE_H_
#define TREENODE_H_

struct TreeNode {
	 int val;
	 struct TreeNode *left;
	 struct TreeNode *right;
};

struct ListNode {
	struct TreeNode * node ;
	struct ListNode next ;
    struct ListNode pre ;
};

#endif /* TREENODE_H_ */
